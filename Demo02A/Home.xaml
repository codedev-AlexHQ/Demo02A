<Window x:Class="Demo02A.Home"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo02A"
        mc:Ignorable="d"
        Title="Home" Height="450" Width="800">
    <Grid>
        
        <StackPanel Orientation="Vertical">
            <Button Content="Operaciones" Width="100" Height="30" Margin="5" x:Name="BtnOperaciones" Click="BtnOperaciones_Click"/>
            <Button Content="Mantenimientos" Width="100" Height="30" Margin="5" x:Name="BtnMantenimientos" Click="BtnMantenimientos_Click"/>
            <Button Content="Reportes" Width="100" Height="30" Margin="5" x:Name="BtnReportes" Click="BtnReportes_Click"/>
        </StackPanel>
    </Grid>
</Window>
