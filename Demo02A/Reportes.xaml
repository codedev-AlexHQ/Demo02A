<Window x:Class="Demo02A.Reportes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Demo02A"
        mc:Ignorable="d"
        Title="Reportes" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Cargas" Width="100" Height="30" Margin="5" x:Name="BtnCargas" Click="BtnCargas_Click"/>
            <Button Content="Ingresos" Width="100" Height="30" Margin="5" x:Name="BtnIngresos" Click="BtnIngresos_Click"/>
            <Button Content="Salidas" Width="100" Height="30" Margin="5" x:Name="BtnSalidas" Click="BtnSalidas_Click"/>
        </StackPanel>

    </Grid>
</Window>
